               <font>
                  <Font size="13.0"/>
               </font>
            </CheckBox>
            <CheckBox fx:id="mentalIllnessCheckBox" contentDisplay="TEXT_ONLY" mnemonicParsing="false"
                      nodeOrientation="RIGHT_TO_LEFT" text="Mental illness">
               <font>
                  <Font size="13.0"/>
               </font>
            </CheckBox>
            <CheckBox fx:id="brucellosisCheckBox" contentDisplay="TEXT_ONLY" mnemonicParsing="false"
