               <font>
                  <Font size="13.0"/>
               </font>
            </CheckBox>
            <CheckBox fx:id="poxCheckBox" contentDisplay="TEXT_ONLY" mnemonicParsing="false"
                      nodeOrientation="RIGHT_TO_LEFT" text="Pox">
               <font>
                  <Font size="13.0"/>
