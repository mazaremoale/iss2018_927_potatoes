               <font>
                  <Font size="13.0"/>
               </font>
            </CheckBox>
            <CheckBox fx:id="cancerCheckBox" contentDisplay="TEXT_ONLY" mnemonicParsing="false"
                      nodeOrientation="RIGHT_TO_LEFT" text="Cancer">
               <font>
                  <Font size="13.0"/>
               </font>
            </CheckBox>
